{
  "name": "api",
  "version": "0.0.1",
  "private": true,
  "main": "./dist/bin/www",
  "scripts": {
    "start": "ts-node ./bin/www",
    "dev": "nodemon",
    "test": "jest --watch --coverage"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "./"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "dependencies": {
    "@types/express": "^4.17.0",
    "@types/node": "^12.0.10",
    "@types/supertest": "^2.0.7",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "should": "^13.2.3",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2"
  }
}
